@model WebApplication1.Models.Contact
@{
    ViewBag.Title = "UpdateContact";
}


<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />

</head>
<body>
    <link href="projet.css" rel="stylesheet" type="text/css" media="all" />
    <h1>Ajout de contacts</h1>
    <h2>
        <form action="updateContact" method="post">
            <p><input type="hidden" value="@Model.id"name="Id" /></p>
            <p>Number <input class="entreText" value="@Model.name" type="text" name="Number" /></p>
            <p>Contact Name <input class="entreText" value="@Model.number" type="text" name="Name" /></p>
            <input class="bouton" type="submit" value="Valider" />
        </form>
    </h2>
</body>
</html>
