@model WebApplication1.Models.compagny
@{
    ViewBag.Title = "Update";
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />

</head>
<body>
    <link href="projet.css" rel="stylesheet" type="text/css" media="all" />
    <h1>Ajout de d'entreprise</h1>
    <h2>
        <form action="update" method="post">
            <p><input type="hidden" value="@Model.id"name="Id" /></p>
            <p>Adress <input class="entreText" value="@Model.adress" type="text" name="Adress" /></p>
            <p>Compagny <input class="entreText" value="@Model.name" type="text" name="Name" /></p>
            <input class="bouton" type="submit" value="Valider" />
        </form>
    </h2>
</body>
</html>
